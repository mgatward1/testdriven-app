{
    "containerDefinitions": [
      {
        "name": "swagger",
        "image": "%s.dkr.ecr.eu-west-2.amazonaws.com/test-driven-swagger:production",
        "essential": true,
  
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "testdriven-swagger-prod",
            "awslogs-region": "eu-west-2"
          }
        },
        "portMappings": [
          {
            "hostPort": 0,
            "protocol": "tcp",
            "containerPort": 8080
          }
        ],
        "environment": [
          {
            "name": "URL",
            "value": "swagger.json"
          }
        ],
        "memoryReservation": 300
      }
    ],
    "family": "testdriven-swagger-prod-td"
  }